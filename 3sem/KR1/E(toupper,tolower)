#include <iostream>
#include <cmath>
#include <string>
#include <cctype>
using namespace std;



int main() {
	string s;
	getline(cin, s);
	int i=0, m=0;
	while (i<s.length()) {
		if (m%2==0) cout << (char) toupper(s[i]);
		else cout << (char) tolower (s[i]);
		if (s[i]!=' ') m++;
		i++;
	}
	
	
	return 0;
}
